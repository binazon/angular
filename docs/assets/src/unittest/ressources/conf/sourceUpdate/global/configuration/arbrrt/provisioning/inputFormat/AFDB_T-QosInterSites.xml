<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>sourceSite</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Name of the source site (phone entity of type site), "Unknown" by default.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>destinationSite</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Name of the destination site (phone entity of type site), "Unknown" by default.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>subset</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Traffic subset. The possible values are : onNet, extOnNet, offNet.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>compression</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Compression. The possible values are G711, G729.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>numberOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				number of calls (just for diag, no relation with QoS statistics)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>numberOfSuccessfulCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				number of successful calls (just for diag, no relation with QoS statistics)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>numberOfCallsTakenIntoAccountForQos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of phone calls taken into account (calls without CMR or with inconsistent latency are not recorded here).
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>numberOfCmrTakenIntoAccount</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of CMR taken into account (CMRs for the calls taken into account).
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>numberOfPackets</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of IP packets transmitted.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>sumOfLatencies</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Sum of latencies.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>sumOfJitters</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Sum of jitters.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>numberOfLostPackets</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of lost packets.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c12</columnName>
        <alias>numberOfCallsInLowRangeOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of calls with a MOS value in range [ 1, MOS_THRESHOLD_1 [
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c13</columnName>
        <alias>numberOfCallsInMediumRangeOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of calls with a MOS value in [ MOS_THRESHOLD_1, MOS_THRESHOLD_2 ]
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c14</columnName>
        <alias>numberOfCallsInHighRangeOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of calls with a MOS value in range ] MOS_THRESHOLD_2, 5 ]
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c15</columnName>
        <alias>averageMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Average MOS (decimal number rounded to 1 fractional digit).
Example: 4.1
Empty if no phone calls could be taken into account (i.e. field numberOfCallsTakenIntoAccountForQos is equal to 0)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c16</columnName>
        <alias>highestValueOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Highest value of MOS (decimal number rounded to 2 fractional digits).
Example: 4.11
Empty if no phone calls could be taken into account (i.e. field numberOfCallsTakenIntoAccountForQos is equal to 0)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c17</columnName>
        <alias>timeOfHighestValueOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Time of the highest value of MOS (format YYYY-MM-DD hh:mm:ss)
This date is the start date of the corresponding call.
Empty if no phone calls could be taken into account (i.e. field numberOfCallsTakenIntoAccountForQos is equal to 0)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c18</columnName>
        <alias>lowestValueOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Lowest value of MOS (decimal number rounded to 2 fractional digits).
Example: 4.11
Empty if no phone calls could be taken into account (i.e. field numberOfCallsTakenIntoAccountForQos is equal to 0)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c19</columnName>
        <alias>timeOfLowestValueOfMos</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Time of the lowest value of MOS (format YYYY-MM-DD hh:mm:ss)
This date is the start date of the corresponding call.
Empty if no phone calls could be taken into account (i.e. field numberOfCallsTakenIntoAccountForQos is equal to 0)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
