<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Date, [0..23] for daily and [1..31] for Monthly
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>Duration</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Duration, In seconds
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>csrstState</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				csrstState, Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>csrstTotalUpTime</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				csrstTotalUpTime, Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
