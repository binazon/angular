<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>siteId</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				-	GK site ID in AFDB_BTG-SITES_D-<YYMMDD>-TopTalkers.txt
-	empty field in "AFDB_BTG-NETWORK_*-TopTalkers.txt"
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>phoneNumber</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Phone number
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>direction</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Direction of traffic from the phone's point of view: either "In" (Incoming traffic) or "Out" (Outgoing traffic).
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>subset</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Main subset of traffic (example : onNet)
Note: the main subset of traffic is first part of the AFDB call type defined in "afdbCallTypesForGkCdr.txt". For example, the main subset of traffic for the subset "onNet_SAN-SA" is onNet.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>callRate</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Call rate compared to the total number of calls on the network for this direction and this traffic subset. This rate (between 0 and 100) is given with 2 decimals, except in the case where it is equal to 100.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>numberOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of calls
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>durationOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Duration of phone calls
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>numberOfSuccessfulCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of phone calls that resulted in communication between the caller and the called party.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>numberOfAcceptedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of phone calls said to be "accepted" (called party busy, called party does not answer, ...)
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>numberOfRejectedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of rejected phone calls.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>numberOfFailedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Number of phone calls that have failed.
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
