<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Date, [0..23] for daily and [1..31] for Monthly
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>DurOfAllPollPrd</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Duration of all polling periods, In seconds
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>SvcType</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Service Type, 
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>SvcDesc</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Service Description, Service description at the end of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>SvcState</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Service State, Service state at the end of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
