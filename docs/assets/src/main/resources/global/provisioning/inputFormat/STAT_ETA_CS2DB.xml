<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Date, [0..23] for daily and [1..31] for Monthly
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>Duration</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Duration, In seconds
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>Status</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Status, Last value of the period 
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>DurOfUpState</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Duration of UP status, Duration where Status=1
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
