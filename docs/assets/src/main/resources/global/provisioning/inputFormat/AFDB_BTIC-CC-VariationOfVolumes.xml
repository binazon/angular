<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of the period, on 4 digits (ex: 0019).
Note :
-	Value in range [0, 23] for the hourly aggregations (hour of the day).
-	Value in range [1, 31] for the daily aggregations (day of the month).
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>siteId</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Destination site Identifier (Site name or “Unknown”).
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>contactCenter</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Identifier of “Contact Center” (ex: 719). 
This is the identifier of “Contact Center” given by “BTIC_CONTACT_CENTERS.txt”/”BTIC-SIP_CONTACT_CENTERS.txt”.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>reserved1</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field, empty
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>receptionNumber</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reception number (ex: 84732). This field can be empty. 
Note: This field is always empty for the upstream statistics.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>subset</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Always empty (these statistics are done without distinction of traffic subset)
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>telephonyZone</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Name of telephony zone (ex: “CHINA SHANGHAI”). This field can be empty.
Calling telephony zone in upstream statistics.
Called telephony zone in downstream statistics.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>reserved2</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field, empty.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>numberOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of calls				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>durationOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Duration of calls, in seconds.				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>numberOfSuccessfulCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that resulted in communication between the caller and the called party.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>numberOfAcceptedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls said to be "accepted" (called party busy, called party does not answer, ...)
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c12</columnName>
        <alias>numberOfRejectedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of rejected phone calls.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c13</columnName>
        <alias>numberOfFailedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that have failed.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
