<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Date, [0..23] for daily and [1..31] for Monthly
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>Duration</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Duration, In seconds
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>NbOfRegisDvc</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Number of registered device, Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>NbOfUregisDvc</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Number of unregistered device, Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>NbOfRejctDvc</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Number of rejected device, Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>NbElt</alias>
        <dataFormat>int</dataFormat>
        <comments><![CDATA[
				Number of Element present in the following list
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>DvcName</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Device name, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>DvcType</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Device Type, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>DvcDesc</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Device Description, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>DvcState</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Device Status, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>DvcIPAddr</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Device IP Address, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>LastChge</alias>
        <dataFormat>void</dataFormat>
        <comments><![CDATA[
				Last Change, Table - Last value of the period
				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
