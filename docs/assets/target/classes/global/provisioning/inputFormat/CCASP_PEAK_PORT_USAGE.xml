<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>ENTITYID</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				EntityId ORIGIN:EQUANT.CCASP_CUSTOMER:<CUSTOMERACCOUNTNO>				
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>DATE</alias>
        <dataFormat>DATETIME</dataFormat>
        <comments><![CDATA[
				Date defined in the filename				
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>PARTNERACCOUNTNO</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				CCA SP Port Subscription account id
				]]></comments>
        <type>H2</type>
    </inputColumn>    
    <inputColumn>
        <columnName>c3</columnName>
        <alias>PARTNERNAME</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Name of CCA SP Partner
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>CUSTOMERACCOUNTNO</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Account id of the end customer
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>CUSTOMERACCOUNTNAME</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Name of the end customer
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>OFFERCODE</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Internal offer code
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>SERVICETYPE</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Service type
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>SERVICEMODEL</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Service model
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>SKUID</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				SKU ID for the ports offer
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>SKULABEL</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Description of SKU
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>MONTH_YEAR</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
				Month and year of peak port usage(ex: NOV-2017)
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c12</columnName>
        <alias>PEAKTIME</alias>
        <dataFormat>DATETIME</dataFormat>
        <comments><![CDATA[
				Date and time of peak port usage. Format : mm/dd/yyyy HH:MM:SS AM (ex: 17-11-2017 09:05:57 PM)
				]]></comments>
        <type>H2</type>
    </inputColumn>
    <inputColumn>
        <columnName>c13</columnName>
        <alias>PEAKPORTS</alias>
        <dataFormat>INTEGER</dataFormat>
        <comments><![CDATA[
				Number of port used during peak port usage for the given site
				]]></comments>
        <type>H2</type>
    </inputColumn>
</listInputColumn>
