<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>periodNumber</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of the period, on 4 digits (ex: 0019).
Note :
-	Value in range [0, 23] for the hourly aggregations (hour of the day).
-	Value in range [1, 31] for the daily aggregations (day of the month).
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>element</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
- Site identifier for the AFDB_BTIC-SITES* files
- Always empty for the AFDB_BTIC-NETWORK* files
- GU identifier for the AFDB_BTIC-GU* files
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>reserved</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
reserved field, empty
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>direction</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Direction of traffic from the customer’s network point of view.
Possible values: “In“ (Incoming traffic), “Out“ (Outgoing traffic), “Int” (Internal).
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>subset</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Subset of traffic (AFDB call type deduced from the configuration file “afdbCallTypesForBticH323Cdr.txt”). 
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>numberOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of calls				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>durationOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Duration of calls, in seconds.				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>numberOfSuccessfulCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that resulted in communication between the caller and the called party.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>numberOfAcceptedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls said to be "accepted" (called party busy, called party does not answer, ...)
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>numberOfRejectedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of rejected phone calls.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>numberOfFailedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that have failed.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>reserved</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
reserved field, empty
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
