<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<listInputColumn>
    <inputColumn>
        <columnName>c0</columnName>
        <alias>siteId</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Destination site identifier (Site name or “Unknown”).
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c1</columnName>
        <alias>reserved1</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c2</columnName>
        <alias>reserved2</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c3</columnName>
        <alias>contactCenter</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Identifier of “Contact Center”  (ex: 719). This is the identifier of “Contact Center” given by “BTIC_CONTACT_CENTERS.txt”.
This is the destination CC in *UpstreamVolumes* files and this is the source CC in *DownstreamVolumes*.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c4</columnName>
        <alias>reserved3</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field, empty 
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c5</columnName>
        <alias>receptionNumber</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reception number (ex: 84732). This field can be empty. 
This field is always empty for the upstream statistics.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c6</columnName>
        <alias>subset</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Subset of traffic identifed by an “AFDB call type” (ex: “offNet”). This field can be empty.				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c7</columnName>
        <alias>telephonyZone</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Name of telephony zone (ex: “CHINA BEIJING”). This field can be empty.
This is the calling zone for the upstream statistics and this is the called zone for the downstream statistics.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c8</columnName>
        <alias>reserved4</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Reserved field, empty.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c9</columnName>
        <alias>numberOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of calls				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c10</columnName>
        <alias>durationOfCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Duration of calls, in seconds.				
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c11</columnName>
        <alias>numberOfSuccessfulCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that resulted in communication between the caller and the called party.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c12</columnName>
        <alias>numberOfAcceptedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls said to be "accepted" (called party busy, called party does not answer, ...)
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c13</columnName>
        <alias>numberOfRejectedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of rejected phone calls.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
    <inputColumn>
        <columnName>c14</columnName>
        <alias>numberOfFailedCalls</alias>
        <dataFormat>VARCHAR(255)</dataFormat>
        <comments><![CDATA[
Number of phone calls that have failed.
				]]></comments>
        <type>DATA</type>
    </inputColumn>
</listInputColumn>
